generator client {
  provider      = "prisma-client-js"
  output        = "../generated/prisma"
  binaryTargets = ["native"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id        Int      @id @default(autoincrement())
  email     String   @unique
  password  String
  name      String?
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}

model EmailDelivery {
  id        String   @id @db.Uuid
  from      String
  subject   String
  strHtml   String?
  status    String?
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
  recipient String
  strText   String?
}
